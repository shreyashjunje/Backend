<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>WhatsApp</title>
  </head>
  <body>
    <h3>ALL CHATS</h3>
    <% for(let chat of chats) { %>
      <div>
        <p>From: <%= chat.from %></p>
        <div>
            <p><%= chat.message %></p>
        </div>
        
        <p>Received by: <%= chat.to %></p>

        <hr>
      </div>
    <% } %>
  </body>
</html>
